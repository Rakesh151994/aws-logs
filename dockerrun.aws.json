{
"AWSEBDockerrunVersion": "2",
    "containerDefinitions": [{
            "name": "mysql",
            "image": "mysql:5.7",
            "essential": true,
            "memory": 256,
            "portMappings": [{
                "hostPort": 3306,
                "containerPort": 3306
            }],
            "environment": [{
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "root"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "user"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "password"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "maker"
                }
            ]
        },
        {
            "name": "rakesh",
            "image": "rakesh1594/rakesh:3.2",
            "essential": true,
            "memory": 256,
            "portMappings": [{
                "hostPort": 5000,
                "containerPort": 5000
            }],
            "links": [
                "mysql"
            ]
        }
    ]
}